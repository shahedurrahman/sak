{
  "name": "S.A.K",
  "description": "Swiss Army Knife",
  "author": "Shahedur Rahman",
  "manifest_version": 2,
  "version": "1.0.3",
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.youtube.com/*",
        "*://outlook.live.com/*",
        "*://portal.azure.com/*"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_idle"
    }
  ]
}